<%- include('partials/header') %>
<div class="container mt-5">

    <h2>Upload Relevant Images</h2>
    <div class="row">
        <div class="col-sm-8">
            <div class="card">
                <div class="card-body">

                    <form action="/image/post/<%= complaintId %>" method="POST" enctype="multipart/form-data">
                        <input type="file" name="image">
                        <button type="submit" class="btn btn-dark">Upload</button>
                    </form>

                </div>
            </div>
        </div>
    </div>

    <h2>Upload Relevant Videos</h2>
    <div class="row">
        <div class="col-sm-8">
            <div class="card">
                <div class="card-body">

                    <form action="/video/post/<%= complaintId %>" method="POST" enctype="multipart/form-data">
                        <input type="file" name="video">
                        <button type="submit" class="btn btn-dark">Upload</button>
                    </form>

                </div>
            </div>
        </div>
    </div>

    <h2>Upload Relevant Audios</h2>
    <div class="row">
        <div class="col-sm-8">
            <div class="card">
                <div class="card-body">

                    <form action="/audio/post/<%= complaintId %>" method="POST" enctype="multipart/form-data">
                        <input type="file" name="audio">
                        <button type="submit" class="btn btn-dark">Upload</button>
                    </form>

                </div>
            </div>
        </div>
    </div>

    <div class="row mt-3">
        <div class="col-sm-8">
            <form action="/user/dashboard" method="GET">
                <button type="submit" class="btn btn-dark">Finish</button>
            </form>
        </div>
    </div>

    </div>
</div>
</body>
</html> 