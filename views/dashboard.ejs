<div class="container mt-5">
    <h1>Dashboard</h1>
    <br><br>

    <form action="/complaint/post" method="GET">
        <button type="submit" class="btn btn-dark">File Complaint</button>
    </form>

    <br>

    <form action="/user/promote" method="GET">
        <button type="submit" class="btn btn-dark">Promote User</button>
    </form>

    <br>

    <form action="/complaint/hotspots" method="GET">
        <button type="submit" class="btn btn-dark">Show Hotspots</button>
    </form>

    <br>

    <form action="/complaint/graphs" method="GET">
        <button type="submit" class="btn btn-dark">Statistical Data</button>
    </form>

    <br>

    <form action="/user/logout" method="GET">
        <button type="submit" class="btn btn-dark">Logout</button>
    </form>

</div>

<script src="https://cdn.jsdelivr.net/npm/@popperjs/core@2.11.7/dist/umd/popper.min.js" integrity="sha384-zYPOMqeu1DAVkHiLqWBUTcbYfZ8osu1Nd6Z89ify25QV9guujx43ITvfi12/QExE" crossorigin="anonymous"></script>
